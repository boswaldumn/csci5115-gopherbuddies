        app:layout_constraintBottom_toTopOf="@+id/userProfilePicture3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/userProfilePicture3" />

    <TextView
        android:id="@+id/courses_list3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
