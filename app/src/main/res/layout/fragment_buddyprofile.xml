                app:layout_constraintEnd_toStartOf="@+id/Buddy_button_schedule"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Buddydivider2" />

